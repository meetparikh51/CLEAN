<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="VO.EventImageVO" table="Event_Image">
   
   	<id name="imageId" type="int" column="Image Id" >
   		<generator class="increment"/>
  	</id>

	<property name="path">
	  		<column name="Path"/>
  	</property>
  
  <property name="name">
  		<column name="Name"/>
  	</property>
  
  	<property name="eventId">
   		<column name="Event Id" />
  	</property>
  
  	
	
 </class>

</hibernate-mapping>
