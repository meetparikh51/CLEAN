<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="VO.ComplainAttachmentVO" table="Complain_Attachment">
   
   	<id name="attachmentId" type="int" column="Attachment Id" >
   		<generator class="increment"/>
  	</id>

  	<property name="attachmentName">
   		<column name="Attachment Name" />
  	</property>
  
  	<property name="path">
  		<column name="Path"/>
  	</property>
  	<property name="complainId">
  		<column name="Complain Id"/>
  	</property>
  
 </class>

</hibernate-mapping>
