<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="VO.CompanyImageAttachmentVO" table="CompanyImage">
   
   	<id name="imageAttachmentId" type="int" column="imageAttachmentId" >
   		<generator class="increment"/>
  	</id>

	<property name="imageName">
	  		<column name="imageName"/>
  	</property>
  
  <property name="path">
  		<column name="path"/>
  	</property>
  
  	<property name="companyRegistrationId">
   		<column name="companyRegistrationId" />
  	</property>
  
 </class>

</hibernate-mapping>
